    {
        "name": "Attach to Chrome",
        "port": 9222,
        "request": "attach",
        "type": "chrome",
        "webRoot": "${workspaceFolder}/web"
    },
        {
            "name": "jashn_e_fabric",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "jashn_e_fabric (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "jashn_e_fabric (release mode)",
            "request": "launch",
